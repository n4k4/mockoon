{
  "name": "mockoon",
  "description": "Mock servers in seconds",
  "version": "0.4.0",
  "author": {
    "name": "Guillaume Monnet",
    "email": "hi@255kb.com",
    "url": "https://255kb.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/255kb/mockoon.git"
  },
  "homepage": "https://mockoon.com",
  "build": {
    "appId": "com.mockoon.app",
    "productName": "Mockoon",
    "directories": {
      "output": "packages",
      "app": "dist"
    },
    "win": {
      "target": "nsis",
      "publisherName": "Guillaume Monnet"
    },
    "nsis": {
      "artifactName": "mockoon.setup.${version}.${ext}"
    },
    "mac": {
      "target": "dmg",
      "type": "distribution",
      "artifactName": "mockoon.setup.${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "category": "Development",
      "icon": "build"
    }
  },
  "scripts": {
    "setversion": "node version-bump.js",
    "dev:electron": "electron ./tmp/serve --serve",
    "dev:app": "webpack --config webpack.config.js --watch",
    "prebuild:app": "rm -rf ./dist",
    "build:app": "webpack --config webpack.config.prod.js",
    "prepackage:win": "rm -rf ./packages",
    "package:win": "electron-builder --win",
    "prepackage:linux": "npm install && rm -rf ./packages",
    "package:linux": "electron-builder --linux",
    "prepackage:mac": "npm install && rm -rf ./packages",
    "package:mac": "electron-builder --mac",
    "publish:win": "node gc-publish.js win",
    "publish:mac": "node gc-publish.js mac",
    "publish:linux": "node gc-publish.js linux",
    "publish:all": "node gc-publish.js all",
    "publish:file": "node gc-publish.js file",
    "build": "webpack",
    "start": "webpack-dev-server --port=4200",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js"
  },
  "dependencies": {
    "@angular/animations": "5.2.5",
    "@angular/common": "5.2.5",
    "@angular/compiler": "5.2.5",
    "@angular/core": "5.2.5",
    "@angular/forms": "5.2.5",
    "@angular/http": "5.2.5",
    "@angular/platform-browser": "5.2.5",
    "@angular/platform-browser-dynamic": "5.2.5",
    "@angular/router": "5.2.5",
    "@ng-bootstrap/ng-bootstrap": "1.0.0",
    "bootstrap": "4.0.0",
    "brace": "0.11.1",
    "core-js": "2.5.3",
    "dummy-json": "2.0.0",
    "electron-is-dev": "0.3.0",
    "electron-json-storage": "4.0.2",
    "electron-window-state": "4.1.1",
    "express": "4.16.2",
    "firebase": "4.10.0",
    "http-proxy-middleware": "0.17.4",
    "killable": "1.0.0",
    "lodash": "4.17.12",
    "material-design-icons": "3.0.1",
    "mime-types": "2.1.18",
    "ng2-ace-editor": "0.3.4",
    "rxjs": "5.5.6",
    "semver": "5.5.0",
    "spinthatshit": "1.0.4",
    "uuid": "3.2.1",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.0",
    "@angular/compiler-cli": "5.2.5",
    "@angular/language-service": "5.2.5",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.4.6",
    "autoprefixer": "^7.2.3",
    "codelyzer": "4.1.0",
    "cssnano": "3.10.0",
    "electron": "1.7.8",
    "electron-builder": "20.10.0",
    "electron-reload": "1.2.2",
    "file-loader": "^1.1.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.10",
    "postcss-url": "^7.1.2",
    "protractor": "5.3.0",
    "raw-loader": "^0.5.1",
    "replace-in-file": "3.1.1",
    "sass-loader": "^6.0.6",
    "script-loader": "0.7.2",
    "source-map-loader": "0.2.3",
    "style-loader": "^0.19.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "url-loader": "^0.6.2",
    "webpack": "~3.11.0",
    "webpack-dev-server": "~2.11.0",
    "html-webpack-plugin": "^2.29.0",
    "circular-dependency-plugin": "^4.2.1",
    "webpack-concat-plugin": "2.4.2",
    "copy-webpack-plugin": "~4.4.1",
    "@angular-devkit/core": "0.3.1",
    "@ngtools/webpack": "1.10.0",
    "postcss-import": "^11.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8"
  }
}
